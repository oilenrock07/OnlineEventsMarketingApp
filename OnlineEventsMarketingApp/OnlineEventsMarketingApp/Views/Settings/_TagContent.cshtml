@model OnlineEventsMarketingApp.Models.Settings.TagListViewModel

<table class="table t1 table-responsive table-striped">
    <tr>
        <th>
            Tag Name
        </th>
        <th>
            Start Date
        </th>
        <th>
            End Date
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model.Tags)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TagName)
            </td>
            <td>
                @item.StartDate.ToShortDateString()
            </td>
            <td>
                @item.EndDate.ToShortDateString()
            </td>
            <td>
                @Html.ActionLink("Edit", "EditTag", new { id = item.TagId }) |
                @Html.ActionLink("Delete", "DeleteTag", new { id = item.TagId  }, new { @class = "js-delete" })
            </td>
        </tr>
    }

</table>