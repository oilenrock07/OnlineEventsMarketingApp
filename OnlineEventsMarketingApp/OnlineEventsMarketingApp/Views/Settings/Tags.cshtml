@model OnlineEventsMarketingApp.Models.Settings.TagListViewModel

@{
    ViewBag.Title = "Tags";
}

<h2>Tag List    </h2>

<div class="row t1">
    <div class="col-xs-12 col-md-6 pull-left">
       @Html.ActionLink("Create New", "CreateTag", new {year = Model.Year, month = Model.Month})
    </div>
    <div class="col-xs-12 col-md-6 pull-right">
        <div class="col-md-6 t1">
            @Html.DropDownListFor(m => m.Year, Model.Years, new {@class = "form-control js-year"})
        </div>
        <div class="col-md-6 t1">
            @Html.DropDownListFor(m => m.Month, Model.Months, new {@class = "form-control js-month" })
        </div>
    </div>
</div>


@Html.Partial("_TagContent", Model)

@section Scripts {
    <script src="~/Scripts/Settings/tags.js" type="text/javascript"></script>
}
